{
  "name": "@edu/server",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "TS backend template powered by tsup",
  "author": "William Nguyen",
  "license": "UNLICENSED",
  "lint-staged": {
    "*.{ts, tsx, js,css,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev": "npm run build && npm start",
    "db:seed": "tsx src/drizzle/seeds/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "build": "tsup",
    "start": "node --enable-source-maps dist/index.js",
    "watch": "tsup --watch",
    "watch:start": "tsup --watch --onSuccess \"npm start\"",
    "lint": "npx @biomejs/biome check src",
    "lint:fix": "npx @biomejs/biome check --apply src",
    "test": "vitest"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm"
    ],
    "sourcemap": true,
    "clean": true,
    "shims": true
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie": "^0.6.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.17.6",
    "@types/nodemailer": "^6.4.15",
    "@types/pg": "^8.11.10",
    "@types/qs": "^6.9.15",
    "@types/slug": "^5.0.8",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.30.4",
    "lefthook": "^1.6.12",
    "tsup": "^8.0.2",
    "tsx": "^4.19.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@bull-board/api": "^5.21.1",
    "@bull-board/fastify": "^5.21.1",
    "@clickhouse/client": "^1.4.0",
    "@directus/sdk": "^17.0.0",
    "@elastic/elasticsearch": "^8.15.1",
    "@fastify/basic-auth": "^5.1.1",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/sensible": "^5.6.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@fastify/under-pressure": "^8.5.0",
    "@scalar/fastify-api-reference": "^1.23.5",
    "@sentry/node": "^8.4.0",
    "@sentry/tracing": "^7.114.0",
    "@sinclair/typebox": "^0.32.31",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.7.8",
    "base-x": "^5.0.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.10.4",
    "clickhouse-migrations": "^1.0.2",
    "cookie": "^0.6.0",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.39.3",
    "fastify": "^4.27.0",
    "fastify-plugin": "^4.5.1",
    "file-type": "^19.6.0",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.1",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.4.2",
    "jsonata": "^2.0.5",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "jwt-decode": "^4.0.0",
    "kafkajs": "^2.2.4",
    "kleur": "^4.1.5",
    "ky": "^1.4.0",
    "lodash": "^4.17.21",
    "mailgen": "^2.0.28",
    "minio": "^8.0.1",
    "mongodb": "^6.8.0",
    "nodemailer": "^6.9.14",
    "pg": "^8.13.1",
    "pino": "^9.1.0",
    "pino-pretty": "^11.1.0",
    "qs": "^6.12.3",
    "sha256": "^0.2.0",
    "sharp": "^0.33.5",
    "short-uuid": "^5.2.0",
    "simple-subtitle-parser": "^2.0.0",
    "slug": "^9.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "subtitles-parser": "^0.0.2",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  }
}
